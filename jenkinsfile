@Library('mylibrary1')_
pipeline
{
    agent any
    stages
    {
        stage('continousdownload_loans')
        {
            steps
            {
                script
            {
                cicd.newDownload("maven")
            }
            }

        }
        stage('continousbuild_loans')
        {
            steps
            {
                script
            {
                cicd.newBuild()
            }
            }

        }
     }
}
